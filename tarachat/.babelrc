{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": false,
        "useBuiltIns": "usage"
      }
    ],
    [
      "@babel/preset-stage-2",
      {
        "loose": false,
        "useBuiltIns": true,
        "decoratorsLegacy": true
      }
    ]
  ],
  "plugins": [
    [
      "@babel/transform-runtime",
      {
        "polyfill": false,
        "regenerator": false
      }
    ]
  ],
  "comments": false
}
